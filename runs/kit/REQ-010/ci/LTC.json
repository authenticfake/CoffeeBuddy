{
  "version": "1.0",
  "req_id": "REQ-010",
  "lane": "infra",
  "env": {
    "PYTHONPATH": "runs/kit/REQ-010/src"
  },
  "tools": {
    "tests": [
      "pytest"
    ],
    "lint": [],
    "types": [],
    "security": [],
    "build": [
      "docker"
    ]
  },
  "commands": {
    "tests": "mkdir -p runs/kit/REQ-010/reports && pytest -q runs/kit/REQ-010/test --junitxml=runs/kit/REQ-010/reports/junit.xml"
  },
  "cases": [
    {
      "name": "tests",
      "run": "mkdir -p runs/kit/REQ-010/reports && pytest -q runs/kit/REQ-010/test --junitxml=runs/kit/REQ-010/reports/junit.xml",
      "cwd": ".",
      "pip-file": "runs/kit/REQ-010/requirements.txt",
      "expect": 0
    }
  ],
  "reports": [
    {
      "kind": "junit",
      "path": "runs/kit/REQ-010/reports/junit.xml",
      "format": "junit-xml"
    }
  ],
  "normalize": {
    "tests": [
      {
        "from": "runs/kit/REQ-010/reports/junit.xml",
        "kind": "junit"
      }
    ]
  },
  "gate_policy": {
    "tests_pass": true,
    "coverage_min": 0.8,
    "security": {
      "bandit_high": 0
    }
  },
  "constraints_applied": [
    "python@3.12",
    "kubernetes",
    "postgres",
    "kafka",
    "prometheus"
  ]
}